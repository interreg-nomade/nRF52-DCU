{
    "configurations": [
        {
            "name": "pca10040/s132",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:NRF_SDK}/**"
            ],
            "defines": [
                "BOARD_PCA10040",
                "BSP_DEFINES_ONLY",
                "CONFIG_GPIO_AS_PINRESET",
                "FLOAT_ABI_HARD",
                "NRF52",
                "NRF52832_XXAA",
                "NRF52_PAN_74",
                "NRF_SD_BLE_API_VERSION=7",
                "S132",
                "SOFTDEVICE_PRESENT",
                "__HEAP_SIZE=8192",
                "__STACK_SIZE=8192"
            ],
            "compilerPath": "${env:GNU_GCC}/arm-none-eabi-gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}